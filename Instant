-- NotificaciÃ³n
local function showNotification(message, color)
    local notif = Instance.new("Frame")
    notif.Size = UDim2.new(0, 380, 0, 90)
    notif.Position = UDim2.new(1, 0, 0, 20)
    notif.BackgroundColor3 = color or Color3.fromRGB(50, 200, 100)
    notif.BorderSizePixel = 0
    notif.ZIndex = 999
    notif.Parent = screenGui
    addCorner(notif, 12)
    
    local notifText = Instance.new("TextLabel")
    notifText.Size = UDim2.new(1, -20, 1, -20)
    notifText.Position = UDim2.new(0, 10, 0, 10)
    notifText.BackgroundTransparency = 1
    notifText.Text = message
    notifText.Font = Enum.Font.GothamBold
    notifText.TextSize = 15
    notifText.TextColor3 = Color3.fromRGB(255, 255, 255)
    notifText.TextWrapped = true
    notifText.ZIndex = 1000
    notifText.Parent = notif
    
    if notifEnabled then
        pcall(function()
            local sound = Instance.new("Sound")
            sound.SoundId = "rbxassetid://6895079853"
            sound.Volume = 0.7
            sound.Parent = notif
            sound:Play()
        end)
    end
    
    notif:TweenPosition(UDim2.new(1, -400, 0, 20), "Out", "Quad", 0.5, true)
    task.spawn(function()
        task.wait(6)
        notif:TweenPosition(UDim2.new(1, 0, 0, 20), "In", "Quad", 0.5, true)
        task.wait(0.5)
        notif:Destroy()
    end)
end

-- ESCANEO MEJORADO con debug
local function scanServer()
    print("\n" .. string.rep("=", 50))
    print("ğŸ” ESCANEANDO SERVIDOR:", game.JobId)
    print(string.rep("=", 50))
    
    local found = {}
    local billboardCount = 0
    local secretCount = 0
    
    -- Buscar en Workspace
    for _, obj in pairs(workspace:GetDescendants()) do
        if obj:IsA("BillboardGui") then
            billboardCount = billboardCount + 1
            
            local hasSecret = false
            local genText = nil
            local nameText = nil
            
            for _, child in pairs(obj:GetDescendants()) do
                if child:IsA("TextLabel") then
                    local text = child.Text
                    
                    -- Debug: mostrar todos los textos
                    if text:len() > 0 then
                        print("  ğŸ“ Label encontrado:", text)
                    end
                    
                    if text:upper():find("SECRET") then
                        hasSecret = true
                        secretCount = secretCount + 1
                        print("  âœ… SECRET detectado!")
                    end
                    
                    if text:match("%$[%d%.]+[KMB]/s") then
                        genText = text
                        print("  ğŸ’° GeneraciÃ³n detectada:", genText)
                    end
                    
                    if not text:find("%$") and not text:find("SECRET") and not text:find("/s") and #text > 2 and #text < 30 then
                        nameText = text
                    end
                end
            end
            
            if hasSecret and genText then
                local num = tonumber(genText:match("([%d%.]+)"))
                local gen = 0
                if num then
                    if genText:find("B/s") then
                        gen = num * 1000000000
                    elseif genText:find("M/s") then
                        gen = num * 1000000
                    elseif genText:find("K/s") then
                        gen = num * 1000
                    end
                end
                
                print("  ğŸ“Š GeneraciÃ³n calculada:", gen, "(mÃ­nimo:", MIN_GENERATION, ")")
                
                if gen >= MIN_GENERATION then
                    table.insert(found, {
                        name = nameText or "Brainrot SECRET",
                        generation = gen,
                        genText = genText,
                        serverId = game.JobId,
                        players = #Players:GetPlayers() .. "/" .. Players.MaxPlayers
                    })
                    print("  âœ… BRAINROT VÃLIDO AGREGADO!")
                else
                    print("  âŒ No cumple mÃ­nimo requerido")
                end
            end
        end
    end
    
    print("\nğŸ“Š RESUMEN DEL ESCANEO:")
    print("  ğŸ–¼ï¸ BillboardGuis encontrados:", billboardCount)
    print("  ğŸ¯ SECRET labels encontrados:", secretCount)
    print("  âœ… Brainrots vÃ¡lidos (>=" .. MIN_GENERATION/1000000 .. "M):", #found)
    print(string.rep("=", 50) .. "\n")
    
    if sortEnabled and #found > 0 then
        table.sort(found, function(a, b) return a.generation > b.generation end)
    end
    
    return found
end

-- ACTUALIZAR LISTA
local function updateResultsList()
    for _, child in pairs(resultsList:GetChildren()) do
        if child:IsA("Frame") then child:Destroy() end
    end
    
    if #goodBrainrots == 0 then
        statusText.Text = string.format("âŒ No se encontraron Brainrots de 10M/s+\nğŸ“Š Servidores revisados: %d\n\nğŸ”„ Sigue explorando...", serversChecked)
        return
    end
    
    if sortEnabled then
        table.sort(goodBrainrots, function(a, b) return a.generation > b.generation end)
    end
    
    for i, brainrot in ipairs(goodBrainrots) do
        local entry = Instance.new("Frame")
        entry.Size = UDim2.new(1, -10, 0, 70)
        entry.Position = UDim2.new(0, 5, 0, (i - 1) * 75)
        entry.BackgroundColor3 = Color3.fromRGB(40, 50, 70)
        entry.BorderSizePixel = 0
        entry.Parent = resultsList
        addCorner(entry, 8)
        
        local icon = Instance.new("TextLabel")
        icon.Size = UDim2.new(0, 50, 0, 50)
        icon.Position = UDim2.new(0, 10, 0, 10)
        icon.BackgroundColor3 = Color3.fromRGB(50, 60, 80)
        icon.BorderSizePixel = 0
        icon.Text = "ğŸ’"
        icon.Font = Enum.Font.GothamBold
        icon.TextSize = 28
        icon.Parent = entry
        addCorner(icon, 8)
        
        local nameLabel = Instance.new("TextLabel")
        nameLabel.Size = UDim2.new(1, -180, 0, 22)
        nameLabel.Position = UDim2.new(0, 70, 0, 8)
        nameLabel.BackgroundTransparency = 1
        nameLabel.Text = brainrot.name
        nameLabel.Font = Enum.Font.GothamBold
        nameLabel.TextSize = 13
        nameLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        nameLabel.TextXAlignment = Enum.TextXAlignment.Left
        nameLabel.TextTruncate = Enum.TextTruncate.AtEnd
        nameLabel.Parent = entry
        
        local genLabel = Instance.new("TextLabel")
        genLabel.Size = UDim2.new(1, -180, 0, 20)
        genLabel.Position = UDim2.new(0, 70, 0, 30)
        genLabel.BackgroundTransparency = 1
        genLabel.Text = "ğŸ’° " .. formatNumber(brainrot.generation) .. "/s"
        genLabel.Font = Enum.Font.GothamBold
        genLabel.TextSize = 12
        genLabel.TextColor3 = Color3.fromRGB(100, 255, 100)
        genLabel.TextXAlignment = Enum.TextXAlignment.Left
        genLabel.Parent = entry
        
        local serverLabel = Instance.new("TextLabel")
        serverLabel.Size = UDim2.new(1, -180, 0, 18)
        serverLabel.Position = UDim2.new(0, 70, 0, 50)
        serverLabel.BackgroundTransparency = 1
        serverLabel.Text = "ğŸ‘¥ " .. brainrot.players
        serverLabel.Font = Enum.Font.Gotham
        serverLabel.TextSize = 11
        serverLabel.TextColor3 = Color3.fromRGB(150, 180, 255)
        serverLabel.TextXAlignment = Enum.TextXAlignment.Left
        serverLabel.Parent = entry
        
        local joinBtn = Instance.new("TextButton")
        joinBtn.Size = UDim2.new(0, 90, 0, 55)
        joinBtn.Position = UDim2.new(1, -100, 0, 7.5)
        joinBtn.BackgroundColor3 = Color3.fromRGB(50, 150, 250)
        joinBtn.Text = "ENTRAR"
        joinBtn.Font = Enum.Font.GothamBold
        joinBtn.TextSize = 14
        joinBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
        joinBtn.Parent = entry
        addCorner(joinBtn, 8)
        
        joinBtn.MouseButton1Click:Connect(function()
            joinBtn.Text = "â³"
            joinBtn.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
            print("ğŸš€ Teleportando a:", brainrot.serverId)
            pcall(function()
                TeleportService:TeleportToPlaceInstance(PLACE_ID, brainrot.serverId, player)
            end)
        end)
        
        joinBtn.MouseEnter:Connect(function()
            if joinBtn.Text == "ENTRAR" then joinBtn.BackgroundColor3 = Color3.fromRGB(70, 170, 255) end
        end)
        joinBtn.MouseLeave:Connect(function()
            if joinBtn.Text == "ENTRAR" then joinBtn.BackgroundColor3 = Color3.fromRGB(50, 150, 250) end
        end)
    end
    
    resultsList.CanvasSize = UDim2.new(0, 0, 0, #goodBrainrots * 75 + 5)
    statusText.Text = string.format("âœ… %d Brainrots listos!\nğŸ’ De 10M/s o mÃ¡s\nğŸ“Š Servidores: %d\n\nğŸ¯ Click ENTRAR", #goodBrainrots, serversChecked)
end

-- EXPLORACIÃ“N
local function startAutoExplore()
    if isExploring then return end
    isExploring = true
    _G.HopperExploring = true
    exploreBtn.Visible = false
    stopBtn.Visible = true
    serversChecked = 0
    
    print("ğŸš€ Iniciando exploraciÃ³n")
    statusText.Text = "ğŸ” Escaneando servidor actual..."
    
    local current = scanServer()
    serversChecked = serversChecked + 1
    
    for _, br in ipairs(current) do
        table.insert(goodBrainrots, br)
    end
    
    if #current > 0 then
        updateResultsList()
        showNotification(string.format("ğŸ‰ %d Brainrot(s) encontrados!\n\nğŸ’ Mejor: %s", #current, formatNumber(current[1].generation) .. "/s"), Color3.fromRGB(50, 200, 100))
    end
    
    task.wait(2)
    
    statusText.Text = "ğŸŒ Obteniendo lista de servidores..."
    local ok, res = pcall(function()
        return HttpService:JSONDecode(game:HttpGet("https://games.roblox.com/v1/games/" .. PLACE_ID .. "/servers/Public?sortOrder=Desc&limit=100"))
    end)
    
    if not ok or not res or not res.data then
        statusText.Text = "âŒ Error al obtener servidores"
        print("âŒ Error HTTP:", res)
        isExploring = false
        _G.HopperExploring = false
        exploreBtn.Visible = true
        stopBtn.Visible = false
        return
    end
    
    local servers = {}
    for _, srv in pairs(res.data) do
        if srv.id ~= game.JobId and srv.playing < srv.maxPlayers and srv.playing > 2 then
            table.insert(servers, srv)
        end
    end
    
    print("âœ… Servidores disponibles:", #servers)
    statusText.Text = string.format("âœ… %d servidores listos\nğŸ“Š Revisados: %d\n\nTeleportando...", #servers, serversChecked)
    task.wait(2)
    
    if #servers > 0 and isExploring and serversChecked < MAX_SERVERS_TO_CHECK then
        local random = servers[math.random(1, math.min(#servers, 10))]
        print("ğŸš€ Teleport a:", random.id, "-", random.playing, "jugadores")
        pcall(function()
            TeleportService:TeleportToPlaceInstance(PLACE_ID, random.id, player)
        end)
    else
        statusText.Text = string.format("â¸ ExploraciÃ³n completada\nğŸ“Š Servidores revisados: %d\nâœ… Brainrots: %d", serversChecked, #goodBrainrots)
        isExploring = false
        _G.HopperExploring = false
        exploreBtn.Visible = true
        stopBtn.Visible = false
    end
end

- POST-TELEPORT
if _G.HopperExploring then
    print("ğŸ”„ Post-teleport detectado")
    statusText.Text = "â³ Cargando servidor...\nEsperando " .. SCAN_TIME .. "s"
    task.wait(SCAN_TIME)
    
    statusText.Text = "ğŸ” Escaneando nuevo servidor..."
    local res = scanServer()
    serversChecked = serversChecked + 1
    
    if #res > 0 then
        for _, br in ipairs(res) do
            table.insert(goodBrainrots, br)
        end
        updateResultsList()
        showNotification(string.format("ğŸ‰ %d Brainrot(s)!\n\nğŸ’ Mejor: %s\nğŸ“Š Servidor %d", #res, formatNumber(res[1].generation) .. "/s", serversChecked), Color3.fromRGB(50, 200, 100))
    end
    
    if isExploring and serversChecked < MAX_SERVERS_TO_CHECK then
        task.wait(2)
        startAutoExplore()
    else
        statusText.Text = string.format("âœ… ExploraciÃ³n finalizada\nğŸ“Š Servidores: %d\nğŸ’ Brainrots: %d", serversChecked, #goodBrainrots)
        isExploring = false
        _G.HopperExploring = false
        exploreBtn.Visible = true
        stopBtn.Visible = false
    end
end

-- BOTONES
scanBtn.MouseButton1Click:Connect(function()
    scanBtn.Text = "â³ ESCANEANDO..."
    scanBtn.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
    
    local res = scanServer()
    goodBrainrots = {}
    for _, br in ipairs(res) do
        table.insert(goodBrainrots, br)
    end
    updateResultsList()
    
    if #res > 0 then
        showNotification(string.format("âœ… %d Brainrot(s)\nğŸ’ Mejor: %s", #res, formatNumber(res[1].generation) .. "/s"), Color3.fromRGB(50, 200, 100))
    else
        showNotification("ğŸ” No se encontraron Brainrots de 10M/s+\nMira la consola (F9) para ver detalles", Color3.fromRGB(255, 150, 50))
    end
    
    scanBtn.Text = "ğŸ” ESCANEAR SERVIDOR ACTUAL"
    scanBtn.BackgroundColor3 = Color3.fromRGB(50, 150, 250)
end)

exploreBtn.
